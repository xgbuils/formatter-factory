{
    "rules": {
        "comma-dangle": [2, "never"],
        
        "no-console": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [2, "functions"],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-trailing-spaces": 2,
        "no-unreachable": 2,
        "no-unused-vars": 2,        
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        "block-scoped-var": 2,
        "curly": 2,
        "eqeqeq": [2, "smart"],
        "indent": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-redeclare": [2, { "builtinGlobals": true }],
        "no-script-url": 2,
        "no-sequences": 2,
        "no-unused-expressions": [2, { allowShortCircuit: true }],
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "yoda": 2,
        "semi": [2, "never"],
        "no-catch-shadow": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "no-undef": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    }
}